<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	t:type="Layout" t:pageTitle="title" 
	t:menuExplanation="menuExplanation">

	<form t:type="Form" t:id="registrationForm">
	   <t:errors/>
           
	   <div class="field">        
	        <t:label for="loginName"/>
	        <span class="entry">    
	            <input t:id="loginName" t:type="TextField"
				       t:validate="required" 
				       size="16" maxlength="30" />
	        </span>
	   </div>
	    
	   <div class="field">        
	        <t:label for="password"/>
	        <span class="entry">
	            <input t:id="password" t:type="PasswordField"
				       t:validate="required" 
	                   size="16" maxlength="8"/>
	        </span>
	    </div>

	   <div class="field">        
	        <t:label for="retypePassword"/>
	        <span class="entry">
	            <input t:id="retypePassword" t:type="PasswordField"
				       t:validate="required" 
	                   size="16" maxlength="8"/>
	        </span>
	    </div>
	    
	   <div class="field">        
	        <t:label for="firstName"/>
	        <span class="entry">    
	            <input t:id="firstName" t:type="TextField"
				       t:validate="required" 
				       size="16" maxlength="30" />
	        </span>
	   </div>
	   
	   <div class="field">        
	        <t:label for="lastName"/>
	        <span class="entry">    
	            <input t:id="lastName" t:type="TextField"
				       t:validate="required" 
				       size="16" maxlength="30" />
	        </span>
	   </div>
	   
	   <div class="field">        
	        <t:label for="email"/>
	        <span class="entry">    
	            <input t:id="email" t:type="TextField"
				       t:validate="required,regexp" 
				       size="16" maxlength="30" />
	        </span>
	   </div>
           
           <div class="field">        
	        <t:label for="dni"/>
	        <span class="entry">    
	            <input t:id="dni" t:type="TextField"
				       t:validate="required" 
				       size="16" maxlength="30" />
	        </span>
	   </div>
            
           <div class="field">        
	        <t:label for="telefono"/>
	        <span class="entry">    
	            <input t:id="telefono" t:type="TextField"
				       t:validate="required" 
				       size="16" maxlength="30" />
	        </span>
	   </div>
           
           <div class="field">        
	        <t:label for="fechaNacimiento"/>
	        <span class="entry">    
	            <input t:id="fechaNacimiento" t:type="TextField"
				       t:validate="required" 
				       size="16" maxlength="30" />
	        </span>
	   </div>
           
           
	    <div class="button">
	        <input type="submit" value="${message:submit}"/>        
	    </div>
	    
	</form>
	
</html>