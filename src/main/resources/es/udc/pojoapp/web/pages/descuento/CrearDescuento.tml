<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	t:type="Layout" t:pageTitle="title" 
	t:menuExplanation="menuExplanation">


<form t:type="Form" t:id="crearDescuentoForm">
	   <t:errors/>
           
             <div class="field">        
	        <t:label for="nombreDescuento"/>
	        <span class="entry">    
	            <input t:id="nombreDescuento" t:type="TextField"
				       t:validate="required" 
				       size="16" maxlength="30" />
	        </span>
	   </div>
           
	   <div class="field">        
	        <t:label for="porcentajeDescuento"/>
	        <span class="entry">    
	            <input t:id="porcentajeDescuento" t:type="TextField"
				       t:validate="required" 
				       size="16" maxlength="30" />
	        </span>
	   </div>
	    	
           
            <div class="button">
	        <input type="submit" value="${message:submit}"/>        
	    </div>
	    
	
</form>  
	  
          

   
</html>